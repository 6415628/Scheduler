Method GetIsLiveImage () declarative remote as String
{
  Description: "Returns the name of the 'live' icon if this is a Live scenario."
  TextBody:
  [*
    image := '';
    if ( this.IsLive() )
    {
      image := 'FACTORY';  
    }
    
    return image;
  *]
}
