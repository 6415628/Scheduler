Function CalcHasCampaign
{
  Description: 'True when the resource group has one or more owned campaign types.'
  TextBody:
  [*
    // James Kwapien Dec-29-2016 (created)
    
    value := this.CampaignType( relsize ) > 0;
    
    this.HasCampaign( value );
  *]
}
