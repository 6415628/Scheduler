Function CalcHasToolRequirement
{
  Description: 'Check if this resource has tool requirement(s).'
  TextBody:
  [*
    // alexandern Nov-25-2015 (created)
    
    value := this.ToolRequirementBase( relsize ) > 0;
    
    this.HasToolRequirement( value );
  *]
}
