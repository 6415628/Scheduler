Method SubscribeToAlwaysAvailableEvent
{
  Description: 'Subscribe to always available event.'
  TextBody:
  [*
    alwaysAvailableEvent   := this.CalendarRegistry().CommonCalendar().GetAlwaysAvailableEvent();
    this.SubscribeToEvent( alwaysAvailableEvent );
  *]
}
