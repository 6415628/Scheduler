Function CalcHasCampaigns
{
  Description: 'If the campaign type has at least one campaign task, this is true.'
  TextBody:
  [*
    // James Kwapien Dec-29-2016 (created)
    
    value := this.CampaignTask( relsize ) > 0;
    
    this.HasCampaigns( value );
  *]
}
