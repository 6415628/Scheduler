Method FixNode (POANode node, POANode dummynode, Boolean hasdummy)
{
  Description: 'Fix the input node and, if the dummy exist, the dummy node'
  TextBody:
  [*
    // Fix the node
    node.IsFixed( true );
    
    // And fix the dummy if it exists
    if( hasdummy )
    {
      dummynode.IsFixed( true );
    }
  *]
}
